[
  {
    "name": "${container_name}",
    "image": "${image}",
    "portMappings": [
      {
        "containerPort": 3000
      }
    ],
    "memoryReservation": 512,
    "environment": [
      {
        "name": "MB_DB_TYPE",
        "value": "postgres"
      },
      {
        "name": "MB_DB_DBNAME",
        "value": "${db_name}"
      },
      {
        "name": "MB_DB_PORT",
        "value": "${db_port}"
      },
      {
        "name": "MB_DB_USER",
        "value": "${db_username}"
      },
      {
        "name": "MB_DB_PASS",
        "value": "${db_password}"
      },
      {
        "name": "MB_DB_HOST",
        "value": "${db_host}"
      }
    ],
    "essential": true
  }
]
